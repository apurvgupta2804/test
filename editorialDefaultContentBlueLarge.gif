<script type="text/javascript">/* <![CDATA[ */
    var WBATM = {};
    WBATM.delegate = {}
    WBATM.delegate.create = function(scope, fn){
        return function(){ 
            fn.call(scope); 
        }
    };
    WBATM.cookie = new function(){
        var d = document;
        return {
            getCookieDate: function(days){
                var dt = new Date();
                dt.setTime(dt.getTime()+(days*24*60*60*1000));
                var e = "; expires="+dt.toGMTString();
                return e;
            },
            getCookie: function (name){
                var rx = new RegExp(name + '=');
                var cs = d.cookie.split(';');
                for(var i=0;i < cs.length;i++) {
                    var c = cs[i];
                    if(c.match(rx)){
                        var pair = c.split('=');
                        return pair[1];
                    }
                }
                return null;
            },
            setCookie: function(name,val,days){
                var expires = this.getCookieDate(days);
                d.cookie = name+"="+val+expires+"; path=/";
            }
        }
    }();
    WBATM.notify = new function(){
        var cName = 'WBATMhide';
        var previewCookie = 'livePreviewToken';  //SET TO "LIVE PREVIEW" SESSION COOKIE NAME    
        var d = document;
        var barEl = d.getElementById('wba-tm-notification-bar');
        var hiddenBarEl = d.getElementById('wba-tm-notification-bar-hidden');
        return {
            init: function(){
                this.setUnloader();
                var c = WBATM.cookie.getCookie(cName);
                if(c==null || c=="false"){
                    this.show();
                }else{
                    this.hide();
                }
            },
            hide: function(){
                barEl.className = 'wba-tm-notification-hidden';
                hiddenBarEl.className = 'wba-tm-notification-visible';
                WBATM.cookie.setCookie(cName, 'true', 1);
            },
            exit: function(){
                barEl.className = 'wba-tm-notification-hidden';
                var redirectUrl = location.href;
                var endPos = redirectUrl.indexOf('?');
                if (endPos != -1) {
                    redirectUrl = redirectUrl.substring(0,endPos);
                }
                var isPostMethod = false;
                if (isPostMethod) {
                    location.pathname = '/?exitLivePreview=true&redirectUrl=' + redirectUrl;
                } else {
    	            location.href = redirectUrl + '?exitLivePreview=true&redirectUrl=' + redirectUrl;     
                }
            },
            show: function(){
                barEl.className = 'wba-tm-notification-visible';
                hiddenBarEl.className = 'wba-tm-notification-hidden';
                WBATM.cookie.setCookie(cName, 'false', 1);
            },
            setUnloader: function(){
                var w = window;
                if (w.addEventListener){
                    w.addEventListener('beforeunload', this.onUnload, false);
                } else if (w.attachEvent){
                    w.attachEvent("onbeforeunload", this.onUnload);
                }           
            },
            onUnload: function(){
                try{
					window.opener.WBATM.preview.onChildUnload();
				}catch(error){}
            }
        };
    }();
    WBATM.notify.init();
/* ]]> */</script>
